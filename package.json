    "raw-loader": "^4.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-intl": "^5.24.3",
    "react-markdown": "^8.0.7",
    "react-moment-proptypes": "^1.8.1",
    "react-query": "^3.34.8",
    "react-redux": "^7.2.6",
    "react-redux-firebase": "^3.11.0",
    "react-redux-toastr": "^7.6.11",
    "redux": "^4.1.2",
