  "description": "Boilerplate project to use from when creating new projects.",
  "main": "pages/index.js",
  "author": "https://github.com/TuuZzee",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "export": "rm -rf .next out && next build && next export",
    "start": "next start",
    "next-lint": "next lint",
    "lint": "./node_modules/.bin/eslint --fix ./",
    "cssLint": "stylelint './**/*.js' --config .stylelintrc --max-warnings=0 ",
    "jsLint": "eslint --fix '**/*.js'",
    "format": "prettier-eslint --write $PWD/'src/**/*.[jt]s?(x)' && eslint --fix .",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
