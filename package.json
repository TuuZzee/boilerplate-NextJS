    "format": "prettier-eslint --write $PWD/'src/**/*.[jt]s?(x)' && eslint --fix .",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "deploy-serverless": "serverless",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
