{
  "presets": ["next/babel"],
  "plugins": [
    [
      "formatjs",
      {
        "idInterpolationPattern": "[sha512:contenthash:base64:6]",
        "ast": true
      }
    ],
    ["styled-components", { "ssr": true, "displayName": true, "preprocess": false }],
    ["styled-jsx/babel", { "optimizeForSpeed": true }]
  ]
}
